{
  "extends": "../../tslint.json",
  "rules": {
    "variable-name": {
      "options": "allow-leading-underscore"
    },
    "no-inferrable-types": false,
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ]
  }
}
