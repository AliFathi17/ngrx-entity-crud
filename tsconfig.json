{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngrx-entity-crud": [
        "dist/ngrx-entity-crud"
      ],
      "ngrx-entity-crud/*": [
        "dist/ngrx-entity-crud/*"
      ],
      "@models": [
        "src/app/main/models"
      ],
      "@models/*": [
        "src/app/main/models/*"
      ],
      "@services": [
        "src/app/main/services"
      ],
      "@services/*": [
        "src/app/main/services/*"
      ]
    }
  }
}
